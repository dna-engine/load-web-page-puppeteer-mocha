{
   "name": "load-web-page-puppeteer-mocha",
   "description": "Minimal example of loading a web page into puppeteer and testing with mocha",
   "version": "1.0.0",
   "license": "MIT",
   "type": "module",
   "module": "spec.js",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/dna-engine/load-web-page-puppeteer-mocha.git"
   },
   "homepage": "https://github.com/dna-engine/load-web-page-puppeteer-mocha",
   "bugs": "https://github.com/dna-engine/load-web-page-puppeteer-mocha/issues",
   "docs": "https://github.com/dna-engine/load-web-page-puppeteer-mocha#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "javascript",
      "localhost",
      "mocha",
      "nodejs",
      "puppeteer",
      "testing",
      "url"
   ],
   "jshintConfig": {
      "esversion": 11,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "mocha": true
   },
   "scripts": {
      "pretest": "jshint . --exclude-path .gitignore",
      "test": "mocha spec*.js --timeout 7000"
   },
   "devDependencies": {
      "assert-deep-strict-equal": "~1.2",
      "jshint": "~2.13",
      "mocha": "~10.4",
      "puppeteer": "~22.8",
      "puppeteer-browser-ready": "~1.3"
   }
}
