{
   "name": "load-web-page-puppeteer-mocha",
   "description": "Minimal example of loading a web page into puppeteer and testing with mocha",
   "version": "1.0.0",
   "license": "MIT",
   "type": "module",
   "main": "spec.js",
   "repository": "github:dna-engine/load-web-page-puppeteer-mocha",
   "homepage": "https://github.com/dna-engine/load-web-page-puppeteer-mocha",
   "bugs": "https://github.com/dna-engine/load-web-page-puppeteer-mocha/issues",
   "docs": "https://github.com/dna-engine/load-web-page-puppeteer-mocha#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "javascript",
      "localhost",
      "mocha",
      "nodejs",
      "puppeteer",
      "testing",
      "url"
   ],
   "jshintConfig": {
      "esversion": 11,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "mocha": true
   },
   "scripts": {
      "pretest": "jshint . --exclude-path .gitignore",
      "test": "mocha spec*.js --timeout 5000"
   },
   "devDependencies": {
      "assert-deep-strict-equal": "~1.0",
      "cheerio": "~1.0.0-rc.12",
      "jshint": "~2.13",
      "mocha": "~10.2",
      "puppeteer": "~19.5",
      "puppeteer-browser-ready": "~0.5"
   }
}
